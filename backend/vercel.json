{
    "version": 2,
    "name": "campus-keeper-server",
    "builds": [
        { "src": "src/app.js", "use": "@vercel/node" },
        { "src": "src/controllers/*.js", "use": "@vercel/node" },
        { "src": "src/routes/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.js"
        }

    ]
}